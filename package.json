{
    "name": "hugo-theme-waterfront",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "hugo": "hugo server -t hugo-theme-waterfront",
        "init": "cp -aR node_modules/font-awesome/fonts static",
        "build:css": "stylus -c src/stylus/import.styl --out static/css/style.css --include-css",
        "watch:css": "stylus -w src/stylus/import.styl --out static/css/style.css --include-css",
        "build:js": "webpack src/js/main.js static/js/bundle.js --progress --colors",
        "watch:js": "webpack src/js/main.js static/js/bundle.js --progress --colors --watch"
    },
    "author": "kajihaya",
    "license": "MIT",
    "devDependencies": {
        "stylus": "^0.54.5",
        "webpack": "^3.10.0"
    },
    "dependencies": {
        "font-awesome": "^4.7.0",
        "highlight.js": "^9.12.0",
        "normalize.css": "^7.0.0"
    }
}
